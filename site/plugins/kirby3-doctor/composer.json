{
    "name": "bnomei/kirby3-doctor",
    "type": "kirby-plugin",
    "version": "1.1.1",
    "description": "Kirby 3 Plugin to check health of your CMS installation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "system",
        "doctor",
        "check",
        "health",
        "audit",
        "security",
        "installation"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/composer-installer": "^1.1",
        "sensiolabs/security-checker": "^5.0",
        "zendframework/zenddiagnostics": "^1.3"
    },
    "scripts": {
        "build": [
            "yarn",
            "yarn run build"
        ]
    }
}
