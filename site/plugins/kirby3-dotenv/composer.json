{
    "name": "bnomei/kirby3-dotenv",
    "type": "kirby-plugin",
    "version": "1.1.2",
    "description": "Kirby 3 Plugin for environment variables from .env",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "dotenv",
        "dotenv-loader",
        "enviroment-variables"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.1.0",
        "vlucas/phpdotenv": "^2.5",
        "getkirby/composer-installer": "^1.1"
    }
}
